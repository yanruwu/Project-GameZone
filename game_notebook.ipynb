    "                    break\n",
    "                else:\n",
    "                    self.print_board()\n",
    "                    print(\"Casilla no v치lida\")\n",
    "            except ValueError:\n",
    "                print(\"Valor no v치lido\")\n",
    "    \n",
    "    def check_winner(self, marker):\n",
    "        for combo in self.winning_combos:\n",
    "            if all(self.board[pos] == marker for pos in combo):\n",
    "                return combo\n",
    "        return None\n",
    "\n",
    "    def jugar(self):\n",
