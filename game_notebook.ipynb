    "           |     |     \n",
    "''')\n",
    "    def get_move(self, playerid, marker):\n",
    "        while True:\n",
    "            try:\n",
    "                pos = int(input(f'Selecciona casilla, {playerid}\\n')) - 1\n",
    "                if pos in [x for x in range(9)] and self.board[pos] == \" \":\n",
